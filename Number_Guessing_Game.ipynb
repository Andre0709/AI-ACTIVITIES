        "import random\n",
        "\n",
        "number = random.randint(1, 100)\n",
        "guess = -1\n",
        "\n",
        "while guess != number:\n",
        "  guess = int(input(\"Enter your guess: \"))\n",
        "  if guess < number:\n",
        "    print(\"Too low!\")\n",
        "  elif guess > number:\n",
        "    print(\"Too high!\")\n",
        "\n",
        "print(f\"Congratulations! You guessed the number {number} correctly!\")"
      ],
